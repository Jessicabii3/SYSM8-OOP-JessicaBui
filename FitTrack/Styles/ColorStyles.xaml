<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <SolidColorBrush x:Key="PrimaryColor" Color="Black"/>

    <SolidColorBrush x:Key="SecondaryColor" Color="LightGray"/>


    <!-- Övriga färger -->
    <Color x:Key="ShadowColor">LightGray</Color>
    <!-- Skuggfärg -->
    <Color x:Key="SecondaryColor2">DarkGray</Color>
    <!-- Mörkgrå färg för sekundär -->
    <Color x:Key="SecondaryColor_">Crimson</Color>

    <Color x:Key="SecondaryColor3">Black</Color>
    <!-- Svart färg -->
    <Color x:Key="SecondaryColor4">White</Color>
    <!-- Vit färg -->

    <!-- Textfärger -->
    <SolidColorBrush x:Key="titleColor1" Color="#E0E1F1"/>
    <!-- Ljus blåaktig ton för titlar -->
    <SolidColorBrush x:Key="titleColor2" Color="#D5CFF5"/>
    <!-- Mjuk lila ton för titlar -->
    <SolidColorBrush x:Key="titleColor3" Color="#BCBEE0"/>
    <!-- Ljus lavendel ton för titlar -->
    <SolidColorBrush x:Key="plainTextColor1" Color="#9497CD"/>
    <!-- Mjuk blå-lila ton för vanlig text -->
    <SolidColorBrush x:Key="plainTextColor2" Color="#7C80C2"/>
    <!-- Mörkare blå-lila ton för vanlig text -->
    <SolidColorBrush x:Key="plainTextColor3" Color="#7376BD"/>
    <!-- Ännu mörkare blå-lila ton för vanlig text -->
    <SolidColorBrush x:Key="BlackBrush" Color="#FF000000"/>

</ResourceDictionary>